<div class="container">
  <div class="row">
    <%= form_with model:@article do |f| %>
      <%= f.label :投稿タイトル %>
      <%= f.text_field :title %><br />
      <%= f.label :投稿本文 %>
      <%= f.text_area :body %><br />
      <%= f.fields_for :images, @article.images.build do |g| %>
        <%= g.label :投稿画像 %>
        <%= g.attachment_field :file, multiple: true %><br />
        <%= g.label :都道府県名 %>
        <%= g.text_field :prefecture %><br />
      <% end %>
      <%= f.submit "投稿", class:"btn btn-success" %>
    <% end %>
  </div>
</div>